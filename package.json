{
  "name": "machine-poker",
  "version": "0.5.1",
  "description": "The library for Machine Poker Competitions",
  "keywords": [
    "competition",
    "poker"
  ],
  "repository": "git://github.com/HackingMondays/MachinePoker.git",
  "author": "Mark Percival <m@mdp.im> (http://markpercival.us)",
  "scripts": {
    "server": "node_modules/.bin/coffee src/server.coffee",
    "server-open": "node_modules/.bin/coffee src/server.coffee --open",
    "test": "rm -rf lib && coffee --compile -o lib/ src/ && mocha --no-colors --compilers coffee:coffee-script test/*_test.coffee",
    "prepublish": "rm -rf lib && coffee --compile -o lib/ src/",
    "postinstall": "./node_modules/bower/bin/bower install",
    "start": "node_modules/.bin/coffee src/server.coffee"
  },
  "devDependencies": {
    "mocha": ">=0.0.0",
    "coffee-script": ">=1.4.0"
  },
  "dependencies": {
    "async": "^0.9.0",
    "binions": ">=0.6.4",
    "body-parser": "^1.10.1",
    "bower": "^1.3.12",
    "cookie-parser": "^1.3.3",
    "express": "^4.10.7",
    "express-session": "^1.10.1",
    "hoyle": ">=0.2.0",
    "morgan": "^1.5.1",
    "open": "0.0.5",
    "optimist": ">=0.3.5",
    "passport": "^0.2.1",
    "passport-local": "^1.0.0",
    "request": ">=2.11.4",
    "socket.io": "^1.3.2"
  },
  "main": "./lib/index.js",
  "engines": {
    "node": "0.8.x"
  },
  "license": "MIT"
}
